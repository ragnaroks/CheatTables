    <CheatEntry>
      <ID>7</ID>
      <Description>"金钱固定 (仅在商店使用,否则可能异常)"</Description>
      <LastState/>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript Async="1">[ENABLE]
assert("AlienShooter.exe"+5C476,29 7E 04)
"AlienShooter.exe"+5C476:
